<?xml version="1.0" encoding="utf-8" ?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:name>Mdnotes for Zotero</em:name>
    <em:description>Export Zotero items and notes to markdown</em:description>
    <em:id>mdnotes@mdnotes.github.io</em:id>
    <em:version>0.1.3</em:version>
    <em:homepageURL>https://github.com/argenos/zotero-mdnotes</em:homepageURL>
    <em:creator>A. Ortega</em:creator>
    <em:updateURL>https://raw.githubusercontent.com/argenos/zotero-mdnotes/master/update.rdf</em:updateURL>
    <em:type>2</em:type>
    <em:targetApplication>
      <Description>
        <em:id>zotero@chnm.gmu.edu</em:id>
        <em:minVersion>5.0.79</em:minVersion>
        <!-- users could change below to 6 for "compatibility"(not real) with Zotero 6.0 -->
        <em:maxVersion>5.0.*</em:maxVersion>
        <!-- <em:maxVersion>6.0.*</em:maxVersion> -->
      </Description>
      <Description>
        <em:id>juris-m@juris-m.github.io</em:id>
        <em:minVersion>5.0.79</em:minVersion>
        <em:maxVersion>5.0.*</em:maxVersion>
        <!-- <em:maxVersion>6.0.*</em:maxVersion> -->
      </Description>
    </em:targetApplication>
  </Description>
</RDF>